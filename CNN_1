      "source": [
        "# check size (should be images X channels X width X height)\n",
        "train_loader.dataset.tensors[0].shape"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "-ilNpmxGxf-x",
        "outputId": "0d40e55f-87ba-46c8-c677-b9930153905a"
